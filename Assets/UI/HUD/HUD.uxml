                <ui:Button text="End" display-tooltip-when-elided="true" name="TutorialEndButton" class="text-Button" style="position: absolute; top: 625px; left: 1542px; height: 100px; width: 170px; background-color: rgb(165, 123, 63); border-left-color: rgb(209, 237, 253); border-right-color: rgb(209, 237, 253); border-top-color: rgb(209, 237, 253); border-bottom-color: rgb(209, 237, 253); border-top-width: 5px; border-right-width: 5px; border-bottom-width: 5px; border-left-width: 5px; color: rgb(204, 230, 247); font-size: 50px; -unity-font-style: bold; -unity-font-definition: resource(&apos;Aetherius&apos;);" />
