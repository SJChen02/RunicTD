            <ui:VisualElement name="TutorialPage1" style="flex-grow: 1; display: flex;">
                <ui:VisualElement style="flex-grow: 1; position: absolute; height: 316px; width: 543px; top: 94px; left: 223px; background-image: resource(&apos;Visuals/Screenshot 2025-01-05 153441&apos;); border-left-color: rgb(209, 237, 253); border-right-color: rgb(209, 237, 253); border-top-color: rgb(209, 237, 253); border-bottom-color: rgb(209, 237, 253); border-top-width: 5px; border-right-width: 5px; border-bottom-width: 5px; border-left-width: 5px;" />
                <ui:VisualElement style="flex-grow: 1; height: 203px; width: 524px; position: absolute; background-image: resource(&apos;Visuals/Screenshot 2025-01-06 071100&apos;); top: 101px; left: 233px;" />
                <ui:Label tabindex="-1" text="This is your mana bar, mana &#10;and your fortress health.&#10;each time your mana bar fills&#10;up your mana will increase.&#10;you can use mana to do all &#10;kinds of magical things" display-tooltip-when-elided="true" class="Tutorial-Text" style="position: absolute; height: 276px; width: 465px; top: 453px; left: 224px;" />
                <ui:VisualElement style="flex-grow: 1; position: absolute; height: 289px; top: 120px; width: 612px; left: 898px; background-image: resource(&apos;Visuals/Screenshot 2025-01-05 153414&apos;); border-left-color: rgb(209, 237, 253); border-right-color: rgb(209, 237, 253); border-top-color: rgb(209, 237, 253); border-bottom-color: rgb(209, 237, 253); border-top-width: 5px; border-right-width: 5px; border-bottom-width: 5px; border-left-width: 5px; display: flex;" />
